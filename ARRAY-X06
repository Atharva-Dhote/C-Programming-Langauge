#include <stdio.h>

void main() {
    int a[5];
    int i,sum=0;
    printf("\n Enter Five Values:");
    for(i=0;i<5;i++)
    {
        scanf("\n %d",&a[1]);
    }
    for(i=0;i<5;i++)
    {
        sum=sum+a[i];
    }
     printf("\n Sum of Element is %d",sum);
     printf("\n Avarage of Elements is %f",sum/5.0);
}
     
    
   
