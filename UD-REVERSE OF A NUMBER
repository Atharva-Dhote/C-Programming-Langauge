#include <stdio.h> 
void reverse() 
{
int num,rev,rem; 
printf("Enter any Number :"); 
scanf("%d",&num); 
rev=0;
while(num!=0) 
{
rem=num%10; 
rev=rev*10+rem; 
num=num/10;
} 
printf( "Reverse of Given Number is : %d",rev); 
}

int main() 
{
printf("Name:-Atharva Dhote\n"); 
printf("Roll.No:-31\n"); 
reverse(); 
} 
