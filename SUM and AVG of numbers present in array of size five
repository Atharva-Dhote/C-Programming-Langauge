#include <stdio.h>

void main() {
    int a[5];
    int i,sum=0,avg;
    printf("\n Enter Five Values:");
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<5;i++)
    {
     sum=sum+a[i];  
     avg=sum/5.0;
    }
    printf("\n Sum=%d", sum);
    printf("\n avg=%f",sum/5.0);
}  
   
