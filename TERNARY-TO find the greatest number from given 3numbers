// Online C compiler to run C program online
#include <stdio.h>

void main() 
{
    int a,b;
    printf("\n Enter any two numbers:");
    scanf("%d %d",&a,&b);
    
      
      (a > b) ? printf("a is greater than b that is %d > %d",
                     a, b)
            : ( (a<b) ? printf("b is greater than a that is %d > %d",
                     b, a)
                     : printf("both are same")
              );
              
}
