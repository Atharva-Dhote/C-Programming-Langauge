#include <stdio.h>

int main() {
  int yr;
    printf("Enter any year:");
    scanf("%d",&yr);
    if(yr%100==0)
    {
        printf("\n century %d is leap yr",yr);
    }
    else 
    {
        if(yr%4==0)
        {
            printf("\n century yr %d is leap yr",yr);
        }
    }
    //else
{
    printf("  \n %d is not century yr",yr);
    printf(" \n %d is not leap yr",yr);
}

   
}
